[
    {
        "id": "1001",
        "name": "FDIC-NEWS-FIN-INST-LETTERS",
        "description": "FDIC Financial Institution Letters",
        "url": "https://fdic.gov/news/financial-institution-letters/index.html",
        "pipeline": [
            {
                "stage": "WEB-PAGE-DOWNLOADER"
            },
            {
                "stage": "HTML-TABLE-PARSER",
                "config": {
                    "tableSelector": "#block-fdic-theme-content .views-row",
                    "columns": [
                        { "name": "publishedOn", "selector": ".news-date time", "attribute": "datetime" },
                        { "name": "title", "selector": ".news-title" },
                        { "name": "summary", "selector": ".news-content" },
                        { "name": "linkToRegChangeText", "selector": ".news-title", "isLink": true }
                    ]
                }
            }
        ],
        "defaults": {
            "source": "FDIC",
            "issuingAuthority": "FDIC",
            "typeOfChange": 3,
            "eventType": 481,
            "regType": 542,
            "identifier": "${publishedOn}",
            "year": "${new Date(publishedOn).getFullYear()}",
            "regulationStatus": 486,
            "topic": "Financial Regulation"
        }
    },
    {
        "id": "1002",
        "name": "SEC-NEWS-PRESS-RELEASES",
        "description": "FDIC Financial Institution Letters",
        "url": "https://www.sec.gov/news/pressreleases.rss",
        "pipeline": [{ "stage": "RSS-PARSER" }],
        "defaults": {
            "source": "SEC",
            "issuingAuthority": "SEC",
            "typeOfChange": 3,
            "eventType": 481,
            "regType": 542,
            "identifier": "${publishedOn}",
            "year": "${new Date(publishedOn).getFullYear()}",
            "regulationStatus": 486,
            "topic": "Financial Regulation"
        }
    },
    {
        "id": "1003",
        "name": "FDIC-NEWS-PRESS-RELEASES",
        "description": "FDIC Press Releases",
        "url": "https://public.govdelivery.com/topics/USFDIC_26/feed.rss",
        "pipeline": [{ "stage": "RSS-PARSER" }],
        "defaults": {
            "source": "SEC",
            "issuingAuthority": "SEC",
            "typeOfChange": 3,
            "eventType": 481,
            "regType": 542,
            "identifier": "${publishedOn}",
            "year": "${new Date(publishedOn).getFullYear()}",
            "regulationStatus": 486,
            "topic": "Financial Regulation"
        }
    },
    {
        "id": "1004",
        "name": "FED-REGISTER-REG-CHANGES",
        "description": "Ded Register",
        "pipeline": [{ "stage": "FED-REGISTER-PARSER" }],
        "defaults": { "source": "FED-REGISTER" }
    }
]
