[
    {
        "id": "BIS-PRESS-RELEASES",
        "title": "BIS BCBS publications",
        "url": "https://www.bis.org/doclist/all_pressrels.rss",
        "pipeline": [{ "stage": "RSS-PARSER" }],
        "defaults": {
            "source": "BIS",
            "issuingAuthority": "BIS",
            "typeOfChange": 3,
            "eventType": 481,
            "regType": 542,
            "identifier": "${publishedOn.split('T')[0]}",
            "year": "${new Date(publishedOn).getFullYear()}",
            "regulationStatus": 486
        }
    },
    {
        "id": "BIS-BCBS-PUBLICATION",
        "title": "BIS BCBS publications",
        "url": "https://www.bis.org/doclist/bcbspubls.rss",
        "pipeline": [{ "stage": "RSS-PARSER" }],
        "defaults": {
            "source": "BIS",
            "issuingAuthority": "BIS",
            "typeOfChange": 3,
            "eventType": 481,
            "regType": 542,
            "identifier": "${publishedOn.split('T')[0]}",
            "year": "${new Date(publishedOn).getFullYear()}",
            "regulationStatus": 486
        }
    },
    {
        "id": "BIS-OTHER-PUBLICATION",
        "title": "BIS Other Publications",
        "url": "https://www.bis.org/doclist/other_publs.rss",
        "pipeline": [{ "stage": "RSS-PARSER" }],
        "defaults": {
            "source": "BIS",
            "issuingAuthority": "BIS",
            "typeOfChange": 3,
            "eventType": 481,
            "regType": 542,
            "identifier": "${publishedOn.split('T')[0]}",
            "year": "${new Date(publishedOn).getFullYear()}",
            "regulationStatus": 486
        }
    },
    {
        "id": "FDIC-NEWS-FIN-INST-LETTERS",
        "title": "FDIC News Financial Institution Letters",
        "url": "https://fdic.gov/news/financial-institution-letters/index.html",
        "pipeline": [
            {
                "stage": "WEB-PAGE-DOWNLOADER"
            },
            {
                "stage": "HTML-TABLE-PARSER",
                "config": {
                    "tableSelector": "#block-fdic-theme-content .views-row",
                    "columns": [
                        { "name": "publishedOn", "selector": ".news-date time", "attribute": "datetime" },
                        { "name": "title", "selector": ".news-title" },
                        { "name": "summary", "selector": ".news-content" },
                        { "name": "linkToRegChangeText", "selector": ".news-title", "isLink": true }
                    ]
                }
            },
            {
                "stage": "SUMMARY-FROM-SITE-LINK-EXTRACTOR",
                "config": { "selector": "h2:contains('Summary:')" }
            }
        ],
        "defaults": {
            "source": "FDIC",
            "issuingAuthority": "FDIC",
            "typeOfChange": 3,
            "eventType": 481,
            "regType": 542,
            "identifier": "${publishedOn.split('T')[0]}",
            "year": "${new Date(publishedOn).getFullYear()}",
            "regulationStatus": 486,
            "topic": "Financial Regulation"
        }
    },
    {
        "id": "FDIC-NEWS-PRESS-RELEASES",
        "title": "FDIC News Press Releases",
        "url": "https://public.govdelivery.com/topics/USFDIC_26/feed.rss",
        "pipeline": [
            {
                "stage": "RSS-PARSER"
            },
            { "stage": "SUMMARY-HTML-TO-TEXT-EXTRACTOR" }
        ],
        "defaults": {
            "source": "FDIC",
            "issuingAuthority": "FDIC",
            "typeOfChange": 3,
            "eventType": 481,
            "regType": 542,
            "identifier": "${publishedOn.split('T')[0]}",
            "year": "${new Date(publishedOn).getFullYear()}",
            "regulationStatus": 486,
            "topic": "Financial Regulation"
        }
    },
    {
        "id": "FRB-NEWS-PRESS-RELEASES",
        "title": "Federal Reserve Bank News Press Releases",
        "url": "https://www.federalreserve.gov/feeds/press_all.xml",
        "pipeline": [{ "stage": "RSS-PARSER" }],
        "defaults": {
            "source": "FRB",
            "issuingAuthority": "FRB",
            "typeOfChange": 3,
            "eventType": 481,
            "regType": 542,
            "identifier": "${publishedOn.split('T')[0]}",
            "year": "${new Date(publishedOn).getFullYear()}",
            "regulationStatus": 486,
            "topic": "Financial Regulation"
        }
    },
    {
        "id": "FINCEN-NEWS",
        "title": "FINCEN News",
        "url": "https://www.fincen.gov/news-room/news",
        "pipeline": [
            {
                "stage": "WEB-PAGE-DOWNLOADER"
            },
            {
                "stage": "HTML-TABLE-PARSER",
                "config": {
                    "tableSelector": ".views-view-grid .views-row",
                    "columns": [
                        { "name": "publishedOn", "selector": ".views-field-field-date-release" },
                        { "name": "title", "selector": ".views-field-title" },
                        { "name": "summary", "selector": ".views-field-body" },
                        { "name": "linkToRegChangeText", "selector": ".views-field-title", "isLink": true }
                    ]
                }
            },
            {
                "stage": "SUMMARY-FROM-SITE-LINK-EXTRACTOR",
                "config": { "selector": ".region.region-content" }
            }
        ],
        "defaults": {
            "source": "FINCEN",
            "issuingAuthority": "FINCEN",
            "typeOfChange": 3,
            "eventType": 481,
            "regType": 542,
            "identifier": "${publishedOn.split('T')[0]}",
            "year": "${new Date(publishedOn).getFullYear()}",
            "regulationStatus": 486,
            "topic": "Financial Regulation"
        }
    },
    {
        "id": "1002",
        "name": "SEC-NEWS-PRESS-RELEASES",
        "description": "FDIC Financial Institution Letters",
        "url": "https://www.sec.gov/news/pressreleases.rss",
        "pipeline": [{ "stage": "RSS-PARSER" }],
        "defaults": {
            "source": "SEC",
            "issuingAuthority": "SEC",
            "typeOfChange": 3,
            "eventType": 481,
            "regType": 542,
            "identifier": "${publishedOn}",
            "year": "${new Date(publishedOn).getFullYear()}",
            "regulationStatus": 486,
            "topic": "Financial Regulation"
        }
    },

    {
        "id": "1004",
        "name": "FED-REGISTER-REG-CHANGES",
        "description": "Fed Register",
        "pipeline": [{ "stage": "FED-REGISTER-PARSER" }],
        "defaults": { "source": "FED-REGISTER" }
    }
]
